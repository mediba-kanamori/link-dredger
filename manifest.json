{
  "manifest_version": 2,
  "name": "Link Dredger",
  "version": "0.1.0",

  "default_locale": "ja",

  "browser_action": {
  },

  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
